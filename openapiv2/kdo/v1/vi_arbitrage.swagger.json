    },
    "vi_arbitrageViStock": {
      "type": "object",
      "properties": {
        "symbol": {
          "type": "string",
          "title": "주식 심볼 (예: \"A005930\")"
        },
        "viSession": {
          "type": "string",
          "title": "VI 세션 종류 (\"vi_static\", \"vi_dynamic\")"
        },
        "viStartTime": {
          "type": "string",
          "format": "date-time",
          "title": "VI 진입 시간"
        },
        "krxLastPrice": {
          "type": "string",
