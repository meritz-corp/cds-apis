        }
      },
      "title": "트리거 발동 이벤트"
    },
    "vi_arbitrageBasisUpdate": {
      "type": "object",
      "properties": {
        "updateTime": {
          "type": "string",
          "format": "date-time",
          "title": "업데이트 시간"
        },
        "symbol": {
          "type": "string",
          "title": "주식 심볼"
        },
        "nxtPrice": {
          "type": "string",
          "format": "int64",
          "title": "NXT 가격"
        },
        "futuresPrice": {
          "type": "string",
          "format": "int64",
          "title": "선물 가격"
        },
        "basisBp": {
          "type": "string",
          "format": "int64",
          "title": "Basis (bp)"
        },
        "thresholdRatio": {
          "type": "number",
          "format": "double",
          "title": "임계값 대비 비율 (%)"
        }
      },
      "title": "Basis 업데이트 (스트리밍용)"
    },
    "vi_arbitrageBasisWarningEvent": {
      "type": "object",
      "properties": {
        "symbol": {
          "type": "string",
          "title": "주식 심볼"
        },
        "currentBasisBp": {
          "type": "string",
          "format": "int64",
          "title": "현재 Basis (bp)"
        },
        "thresholdBp": {
          "type": "string",
          "format": "int64",
          "title": "임계값 (bp)"
        },
        "thresholdRatio": {
          "type": "number",
          "format": "double",
          "title": "임계값 대비 비율 (%)"
        }
      },
      "title": "Basis 경고 이벤트 (임계값의 80% 이상일 때)"
    },
    "vi_arbitrageListTriggerHistoryResponse": {
      "type": "object",
      "properties": {
        "triggers": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/vi_arbitrageTriggerRecord"
          },
          "title": "트리거 이력"
        },
        "nextPageToken": {
          "type": "string",
          "title": "다음 페이지 토큰"
        },
        "totalCount": {
          "type": "integer",
          "format": "int32",
          "title": "총 건수"
        }
      }
    },
    "vi_arbitrageListViStocksResponse": {
      "type": "object",
      "properties": {
        "viStocks": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/vi_arbitrageViStock"
          },
          "title": "VI 종목 목록"
        },
        "nextPageToken": {
          "type": "string",
          "title": "다음 페이지 토큰"
        }
      }
    },
    "vi_arbitrageTriggerDirection": {
      "type": "string",
      "enum": [
        "TRIGGER_DIRECTION_UNSPECIFIED",
        "TRIGGER_DIRECTION_LONG",
        "TRIGGER_DIRECTION_SHORT"
      ],
      "default": "TRIGGER_DIRECTION_UNSPECIFIED",
      "description": "- TRIGGER_DIRECTION_LONG: NXT 매수, 선물 매도 (NXT \u003c 선물)\n - TRIGGER_DIRECTION_SHORT: NXT 매도, 선물 매수 (NXT \u003e 선물)",
      "title": "트리거 방향"
    },
    "vi_arbitrageTriggerRecord": {
      "type": "object",
      "properties": {
        "id": {
          "type": "string",
          "format": "int64",
          "title": "트리거 ID (auto-increment)"
        },
        "symbol": {
          "type": "string",
          "title": "주식 심볼"
        },
        "futuresSymbol": {
          "type": "string",
          "title": "주식선물 심볼"
        },
        "nxtPrice": {
          "type": "string",
          "format": "int64",
          "title": "트리거 시점 NXT 가격"
        },
        "futuresPrice": {
          "type": "string",
          "format": "int64",
          "title": "트리거 시점 선물 가격"
        },
        "basisBp": {
          "type": "string",
          "format": "int64",
          "title": "트리거 시점 Basis (bp)"
        },
        "direction": {
          "$ref": "#/definitions/vi_arbitrageTriggerDirection",
          "title": "트리거 방향 (LONG: NXT 매수/선물 매도, SHORT: NXT 매도/선물 매수)"
        },
        "triggerTime": {
          "type": "string",
          "format": "date-time",
          "title": "트리거 시간"
        },
        "result": {
          "$ref": "#/definitions/vi_arbitrageTriggerResult",
          "title": "트리거 결과 (주문 성공 여부)"
        },
        "errorMessage": {
          "type": "string",
          "title": "에러 메시지 (실패 시)"
        }
      },
      "title": "VI Arbitrage 트리거 이력"
    },
    "vi_arbitrageTriggerResult": {
      "type": "string",
      "enum": [
        "TRIGGER_RESULT_UNSPECIFIED",
        "TRIGGER_RESULT_SUCCESS",
        "TRIGGER_RESULT_FAILED",
        "TRIGGER_RESULT_SKIPPED"
      ],
      "default": "TRIGGER_RESULT_UNSPECIFIED",
      "description": "- TRIGGER_RESULT_SUCCESS: 주문 성공\n - TRIGGER_RESULT_FAILED: 주문 실패\n - TRIGGER_RESULT_SKIPPED: 스킵됨 (쿨다운 등)",
      "title": "트리거 결과"
    },
    "vi_arbitrageViArbitrageStatus": {
      "type": "object",
      "properties": {
        "enabled": {
          "type": "boolean",
          "title": "서비스 활성화 여부"
        },
        "basisThresholdBp": {
          "type": "string",
          "format": "int64",
          "title": "Basis 임계값 (bp 단위)"
        },
        "viStockCount": {
          "type": "integer",
          "format": "int32",
          "title": "현재 VI 종목 수"
        },
        "triggerCountToday": {
          "type": "integer",
          "format": "int32",
