{
  "swagger": "2.0",
  "info": {
    "title": "kdo/v1/vi_arbitrage.proto",
    "version": "version not set"
  },
  "tags": [
    {
      "name": "ViArbitrageService"
    }
  ],
  "consumes": [
    "application/json"
  ],
  "produces": [
    "application/json"
  ],
  "paths": {
    "/v1/vi-arbitrage/basis:stream": {
      "get": {
        "summary": "VI 종목 Basis 실시간 스트리밍",
        "operationId": "ViArbitrageService_StreamBasis",
        "responses": {
          "200": {
            "description": "A successful response.(streaming responses)",
            "schema": {
              "type": "object",
              "properties": {
                "result": {
                  "$ref": "#/definitions/vi_arbitrageBasisUpdate"
                },
                "error": {
                  "$ref": "#/definitions/rpcStatus"
                }
              },
              "title": "Stream result of vi_arbitrageBasisUpdate"
            }
          },
          "default": {
            "description": "An unexpected error response.",
            "schema": {
              "$ref": "#/definitions/rpcStatus"
            }
          }
        },
        "parameters": [
          {
            "name": "symbols",
            "description": "특정 심볼만 구독 (없으면 현재 VI 종목 전체)",
            "in": "query",
            "required": false,
            "type": "array",
            "items": {
              "type": "string"
            },
            "collectionFormat": "multi"
          },
          {
            "name": "minIntervalMs",
            "description": "업데이트 최소 간격 (ms, 기본: 100)",
            "in": "query",
            "required": false,
            "type": "integer",
            "format": "int32"
          }
        ],
        "tags": [
          "ViArbitrageService"
        ]
      }
    },
    "/v1/vi-arbitrage/events:stream": {
      "get": {
        "summary": "VI 이벤트 실시간 스트리밍 (VI 진입/해제, 트리거 등)",
        "operationId": "ViArbitrageService_StreamViEvents",
        "responses": {
          "200": {
            "description": "A successful response.(streaming responses)",
            "schema": {
              "type": "object",
              "properties": {
                "result": {
                  "$ref": "#/definitions/vi_arbitrageViEvent"
                },
                "error": {
                  "$ref": "#/definitions/rpcStatus"
                }
              },
              "title": "Stream result of vi_arbitrageViEvent"
            }
          },
          "default": {
            "description": "An unexpected error response.",
            "schema": {
              "$ref": "#/definitions/rpcStatus"
            }
          }
        },
        "parameters": [
          {
            "name": "symbols",
            "description": "특정 심볼만 구독 (없으면 전체)",
            "in": "query",
            "required": false,
            "type": "array",
            "items": {
              "type": "string"
            },
            "collectionFormat": "multi"
          },
          {
            "name": "includeBasisWarning",
            "description": "Basis 경고 이벤트 포함 여부 (기본: true)",
            "in": "query",
            "required": false,
            "type": "boolean"
          }
        ],
        "tags": [
          "ViArbitrageService"
        ]
      }
    },
    "/v1/vi-arbitrage/status": {
      "get": {
        "summary": "서비스 상태 조회 (활성화 여부, 설정 등)",
        "operationId": "ViArbitrageService_GetStatus",
        "responses": {
          "200": {
            "description": "A successful response.",
            "schema": {
              "$ref": "#/definitions/vi_arbitrageViArbitrageStatus"
            }
          },
          "default": {
            "description": "An unexpected error response.",
            "schema": {
              "$ref": "#/definitions/rpcStatus"
            }
          }
        },
        "tags": [
          "ViArbitrageService"
        ]
      }
    },
    "/v1/vi-arbitrage/triggers": {
      "get": {
        "summary": "VI 트리거 이력 조회",
        "operationId": "ViArbitrageService_ListTriggerHistory",
        "responses": {
          "200": {
            "description": "A successful response.",
            "schema": {
              "$ref": "#/definitions/vi_arbitrageListTriggerHistoryResponse"
            }
          },
          "default": {
            "description": "An unexpected error response.",
            "schema": {
              "$ref": "#/definitions/rpcStatus"
            }
          }
        },
        "parameters": [
          {
            "name": "pageSize",
            "description": "페이지 크기 (optional, default: 50)",
            "in": "query",
            "required": false,
            "type": "integer",
            "format": "int32"
          },
          {
            "name": "pageToken",
            "description": "페이지 토큰",
            "in": "query",
            "required": false,
            "type": "string"
          },
          {
            "name": "startTime",
            "description": "시작 시간 필터 (이후)",
            "in": "query",
            "required": false,
            "type": "string",
            "format": "date-time"
          },
          {
            "name": "endTime",
            "description": "종료 시간 필터 (이전)",
            "in": "query",
            "required": false,
            "type": "string",
            "format": "date-time"
          },
          {
            "name": "symbol",
            "description": "심볼 필터 (특정 종목만)",
            "in": "query",
            "required": false,
            "type": "string"
          }
        ],
        "tags": [
          "ViArbitrageService"
        ]
      }
    },
    "/v1/vi-arbitrage/vi-stocks": {
      "get": {
        "summary": "현재 VI 종목 목록 조회",
        "operationId": "ViArbitrageService_ListViStocks",
        "responses": {
          "200": {
            "description": "A successful response.",
            "schema": {
              "$ref": "#/definitions/vi_arbitrageListViStocksResponse"
            }
          },
          "default": {
            "description": "An unexpected error response.",
            "schema": {
              "$ref": "#/definitions/rpcStatus"
            }
          }
        },
        "parameters": [
          {
            "name": "pageSize",
            "description": "페이지 크기 (optional, default: 100)",
            "in": "query",
            "required": false,
            "type": "integer",
            "format": "int32"
          },
          {
            "name": "pageToken",
            "description": "페이지 토큰",
            "in": "query",
            "required": false,
            "type": "string"
          }
        ],
        "tags": [
          "ViArbitrageService"
        ]
      }
    }
  },
  "definitions": {
    "protobufAny": {
      "type": "object",
      "properties": {
        "@type": {
          "type": "string"
        }
      },
      "additionalProperties": {}
    },
    "rpcStatus": {
      "type": "object",
      "properties": {
        "code": {
          "type": "integer",
          "format": "int32"
        },
        "message": {
          "type": "string"
        },
        "details": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/protobufAny"
          }
        }
      }
    },
    "v1vi_arbitrageTriggeredEvent": {
      "type": "object",
      "properties": {
        "trigger": {
