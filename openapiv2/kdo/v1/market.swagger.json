    "marketRawMarketMessage": {
      "type": "object",
      "properties": {
        "data": {
          "type": "string",
          "format": "byte",
          "title": "Raw binary data received from the socket"
        },
        "receiveTimestampNs": {
          "type": "string",
          "format": "int64",
          "title": "Timestamp when the message was received (nanoseconds since epoch)"
        },
        "sequenceNumber": {
          "type": "string",
          "format": "uint64",
          "title": "Message sequence number (per socket)"
        }
      },
      "title": "Raw market message received from UDP socket"
    },
    "marketSessionId": {
      "type": "string",
      "enum": [
        "SESSION_ID_UNSPECIFIED",
        "PREVIOUS",
        "CONNECTED",
        "OPENING_ONE_PRICE",
        "ONE_PRICE",
        "CLOSING_ONE_PRICE",
        "VI_ONE_PRICE",
        "VI_OPENING_ONE_PRICE",
        "VI_CLOSING_ONE_PRICE",
        "UNIT_TRADE",
        "POST_MARKET",
        "AUCTION_BID",
        "AUCTION_ASK",
        "SUSPENDED",
        "SHUTDOWN",
        "CLOSED",
        "ETC"
