  "definitions": {
    "commonTradingSession": {
      "type": "string",
      "enum": [
        "TRADING_SESSION_UNSPECIFIED",
        "TRADING_SESSION_PRE_MARKET",
        "TRADING_SESSION_OPENING_AUCTION",
        "TRADING_SESSION_REGULAR",
        "TRADING_SESSION_CLOSING_AUCTION",
        "TRADING_SESSION_CLOSED"
      ],
      "default": "TRADING_SESSION_UNSPECIFIED",
      "description": "- TRADING_SESSION_PRE_MARKET: 장 시작 전\n - TRADING_SESSION_OPENING_AUCTION: 시가 동시호가 (08:30 ~ 09:00)\n - TRADING_SESSION_REGULAR: 장중 (09:00 ~ 15:20)\n - TRADING_SESSION_CLOSING_AUCTION: 종가 동시호가 (15:20 ~ 15:30)\n - TRADING_SESSION_CLOSED: 장 종료",
      "title": "거래 세션 타입"
    },
    "marketAddRawMessagesSocketRequest": {
      "type": "object",
      "properties": {
        "multicastAddress": {
          "type": "string",
          "title": "Multicast address to listen on (e.g., \"239.1.1.1\")"
        },
        "multicastInterface": {
          "type": "string",
          "title": "Interface address to bind (e.g., \"192.168.1.100\")"
        },
        "port": {
          "type": "integer",
          "format": "int64",
          "title": "UDP port to listen on"
        },
        "bufferSize": {
          "type": "integer",
          "format": "int64",
          "title": "Buffer size for receiving UDP packets (default: 2048)"
        }
      },
      "title": "Request to add a new raw UDP socket"
    },
    "marketAddRawMessagesSocketResponse": {
      "type": "object",
      "properties": {
        "success": {
          "type": "boolean",
          "title": "Whether the socket was added successfully"
        },
        "errorMessage": {
          "type": "string",
          "title": "Error message if success is false"
        }
      },
      "title": "Response from adding a raw socket"
    },
    "marketEtfNav": {
