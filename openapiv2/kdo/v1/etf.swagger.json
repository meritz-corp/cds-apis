{
  "swagger": "2.0",
  "info": {
    "title": "kdo/v1/etf.proto",
    "version": "version not set"
  },
  "tags": [
    {
      "name": "EtfService"
    }
  ],
  "consumes": [
    "application/json"
  ],
  "produces": [
    "application/json"
  ],
  "paths": {
    "/v1/etfs": {
      "get": {
        "operationId": "EtfService_ListEtfs",
        "responses": {
          "200": {
            "description": "A successful response.",
            "schema": {
              "$ref": "#/definitions/etfListEtfsResponse"
            }
          },
          "default": {
            "description": "An unexpected error response.",
            "schema": {
              "$ref": "#/definitions/rpcStatus"
            }
          }
        },
        "parameters": [
          {
            "name": "pageSize",
            "description": "페이지 크기 (optional)",
            "in": "query",
            "required": false,
            "type": "integer",
            "format": "int64"
          },
          {
            "name": "pageToken",
            "description": "페이지 토큰 (optional, for pagination)",
            "in": "query",
            "required": false,
            "type": "string"
          },
          {
            "name": "filter",
            "description": "필터링 조건 (optional, AIP-160)",
            "in": "query",
            "required": false,
            "type": "string"
          }
        ],
        "tags": [
          "EtfService"
        ]
      }
    },
    "/v1/etfs:create_redeem": {
      "post": {
        "operationId": "EtfService_CreateRedeemEtf",
        "responses": {
          "200": {
            "description": "A successful response.",
            "schema": {
              "$ref": "#/definitions/etfEtf"
            }
          },
          "default": {
            "description": "An unexpected error response.",
            "schema": {
              "$ref": "#/definitions/rpcStatus"
            }
          }
        },
        "parameters": [
          {
            "name": "body",
            "in": "body",
            "required": true,
            "schema": {
              "$ref": "#/definitions/etfCreateRedeemEtfRequest"
            }
          }
        ],
        "tags": [
          "EtfService"
        ]
      }
    },
    "/v1/{etf}": {
      "get": {
        "operationId": "EtfService_GetEtf",
        "responses": {
          "200": {
            "description": "A successful response.",
            "schema": {
              "$ref": "#/definitions/etfEtf"
            }
          },
          "default": {
            "description": "An unexpected error response.",
            "schema": {
              "$ref": "#/definitions/rpcStatus"
            }
          }
        },
        "parameters": [
          {
            "name": "etf",
            "in": "path",
            "required": true,
            "type": "string",
            "pattern": "etfs/[^/]+"
          }
        ],
        "tags": [
          "EtfService"
        ]
      }
    },
    "/v1/{etf}/tick_impact": {
      "get": {
        "summary": "구성종목 1틱 변동 시 ETF NAV 영향 조회",
        "operationId": "EtfService_GetEtfTickImpact",
        "responses": {
          "200": {
            "description": "A successful response.",
            "schema": {
              "$ref": "#/definitions/etfEtfTickImpact"
            }
          },
          "default": {
            "description": "An unexpected error response.",
            "schema": {
              "$ref": "#/definitions/rpcStatus"
            }
          }
        },
        "parameters": [
          {
            "name": "etf",
            "description": "ETF 리소스 이름 (예: etfs/069500)",
            "in": "path",
            "required": true,
            "type": "string",
            "pattern": "etfs/[^/]+"
          },
          {
            "name": "constituentSymbol",
            "description": "구성종목 심볼",
            "in": "query",
            "required": true,
            "type": "string"
          },
          {
            "name": "basePrice",
            "description": "기준가 (이 가격을 중심으로 위아래 틱 계산)",
            "in": "query",
            "required": true,
            "type": "string"
          },
          {
            "name": "tickRange",
            "description": "위아래 계산할 틱 수 (예: 3이면 base_price 기준 -3틱 ~ +3틱)",
            "in": "query",
            "required": true,
            "type": "integer",
            "format": "int32"
          }
        ],
        "tags": [
          "EtfService"
        ]
      }
    }
  },
  "definitions": {
    "commonProductType": {
      "type": "string",
      "enum": [
        "PRODUCT_TYPE_UNSPECIFIED",
        "PRODUCT_TYPE_STOCK",
        "PRODUCT_TYPE_FUTURES",
        "PRODUCT_TYPE_ETF",
        "PRODUCT_TYPE_CASH"
      ],
      "default": "PRODUCT_TYPE_UNSPECIFIED",
      "title": "상품 타입"
    },
    "etfConversion": {
      "type": "object",
      "properties": {
        "symbol": {
          "type": "string",
          "title": "변환 대상 심볼"
        },
        "productType": {
          "$ref": "#/definitions/commonProductType",
          "title": "변환 대상 상품 타입"
        },
        "ratioPerCu": {
          "type": "number",
          "format": "double",
          "title": "1 CU당 변환 비율\n예: KODEX 2X Inverse → 코스피200 선물 = -0.37"
        }
      },
      "title": "ETF가 다른 상품으로 변환될 수 있는 정보\n예: KODEX 2X Inverse는 1cu당 -0.37개의 코스피200 선물로 변환 가능"
    },
    "etfCreateRedeemEtfRequest": {
      "type": "object",
      "properties": {
        "etf": {
          "type": "string"
        },
        "fund": {
          "type": "string"
        },
        "quantity": {
          "type": "string",
          "format": "int64"
        },
        "isCreation": {
          "type": "boolean"
        }
      },
      "required": [
        "etf",
        "fund",
        "quantity",
        "isCreation"
      ]
    },
    "etfEtf": {
      "type": "object",
      "properties": {
        "id": {
          "type": "string",
          "format": "uint64",
          "title": "ETF ID"
        },
        "symbol": {
          "type": "string",
          "title": "ETF 심볼 (ISIN 코드)"
        },
        "code": {
          "type": "string",
          "title": "ETF 코드"
        },
        "name": {
          "type": "string",
          "title": "ETF 이름"
        },
        "prevClose": {
          "type": "string",
          "title": "마지막 가격"
        },
        "prevCloseNav": {
          "type": "string"
        },
        "lastPrice": {
          "type": "string"
        },
        "lastAskInav": {
          "type": "string"
        },
        "lastBidInav": {
          "type": "string"
        },
        "constituents": {
          "type": "object",
          "additionalProperties": {
            "$ref": "#/definitions/etfEtfPdfConstituent"
          },
          "title": "PDF 구성 종목 목록"
        },
        "creationUnit": {
          "type": "string",
          "format": "int64",
          "title": "설정 단위"
        },
        "replicationMethod": {
          "$ref": "#/definitions/etfReplicationMethod",
          "title": "복제 방법"
        },
        "tickSize": {
          "type": "string",
          "format": "int64",
          "title": "Tick 크기 (원 단위, i64)"
        },
        "listedQuantity": {
          "type": "string",
          "format": "int64",
          "title": "Tick 크기 (원 단위, i64)"
        },
        "leverage": {
          "type": "number",
          "format": "float"
        },
        "tradable": {
          "type": "boolean"
        },
        "shortSellable": {
          "type": "boolean"
        },
        "underlyingAsset": {
          "$ref": "#/definitions/etfUnderlyingAsset",
          "title": "기초자산 정보"
        },
        "cashCreditable": {
          "type": "boolean"
        },
        "cashCreationAmount": {
          "type": "string",
          "format": "int64"
        },
        "conversions": {
          "type": "object",
          "additionalProperties": {
            "$ref": "#/definitions/etfConversion"
          },
          "title": "ETF 변환 정보 (예: 레버리지/인버스 ETF의 선물 변환)"
        }
      }
    },
    "etfEtfPdfConstituent": {
      "type": "object",
      "properties": {
        "symbol": {
          "type": "string",
          "description": "symbol",
          "title": "종목 코드"
        },
        "name": {
          "type": "string",
          "title": "종목명"
        },
        "productType": {
          "$ref": "#/definitions/commonProductType",
          "title": "상품 타입"
        },
        "quantity": {
          "type": "string",
          "format": "int64",
          "title": "구성 수량 (선물 숏의 경우 음수)"
        }
      },
      "title": "PDF 구성 종목"
    },
    "etfEtfTickImpact": {
      "type": "object",
      "properties": {
        "etfSymbol": {
          "type": "string",
          "title": "ETF 심볼"
        },
        "constituentSymbol": {
          "type": "string",
          "title": "구성종목 심볼"
        },
        "currentNav": {
          "type": "string",
          "title": "현재 ETF NAV"
        },
        "currentPrice": {
          "type": "string",
          "title": "구성종목 현재가"
        },
        "points": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/etfTickNavPoint"
          },
          "title": "틱 변동별 NAV 시뮬레이션 결과 (-tick_range ~ +tick_range 순서)"
        }
      },
      "title": "ETF 틱 임팩트 응답"
    },
    "etfListEtfsResponse": {
      "type": "object",
      "properties": {
        "etfs": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/etfEtf"
          },
          "title": "ETF 목록"
        },
        "nextPageToken": {
          "type": "string",
          "title": "다음 페이지 토큰"
        }
      }
    },
    "etfReplicationMethod": {
      "type": "string",
      "enum": [
        "REPLICATION_METHOD_UNSPECIFIED",
        "REPLICATION_METHOD_ACTIVE",
        "REPLICATION_METHOD_PHYSICAL",
        "REPLICATION_METHOD_SYNTHETIC"
      ],
      "default": "REPLICATION_METHOD_UNSPECIFIED",
      "title": "복제 방법"
    },
    "etfTickNavPoint": {
      "type": "object",
      "properties": {
        "tickOffset": {
          "type": "integer",
          "format": "int32",
          "title": "틱 오프셋 (예: -3, -2, -1, 0, +1, +2, +3)"
        },
        "price": {
          "type": "string",
          "title": "해당 틱에서의 구성종목 가격"
        },
        "nav": {
          "type": "string",
          "title": "해당 틱에서의 ETF NAV"
        }
      },
      "title": "틱 변동에 따른 NAV 포인트"
    },
    "etfUnderlyingAsset": {
      "type": "object",
      "properties": {
        "future": {
          "$ref": "#/definitions/etfUnderlyingFuture",
          "title": "선물형"
        },
        "fixedIncome": {
          "$ref": "#/definitions/etfUnderlyingFixedIncome",
          "title": "채권형"
        },
        "commodity": {
          "$ref": "#/definitions/etfUnderlyingCommodity",
          "title": "원자재형"
        },
        "currency": {
          "$ref": "#/definitions/etfUnderlyingCurrency",
          "title": "통화형"
        }
      },
      "title": "기초자산"
    },
    "etfUnderlyingCommodity": {
      "type": "object",
      "properties": {
        "symbol": {
          "type": "string",
          "title": "종목 심볼"
        }
      },
      "title": "원자재형 기초자산"
    },
    "etfUnderlyingCurrency": {
      "type": "object",
      "properties": {
        "symbol": {
          "type": "string",
          "title": "종목 심볼"
        }
      },
      "title": "통화형 기초자산"
    },
    "etfUnderlyingFixedIncome": {
      "type": "object",
      "properties": {
        "symbol": {
          "type": "string",
          "title": "종목 심볼"
        }
      },
      "title": "채권형 기초자산"
    },
    "etfUnderlyingFuture": {
      "type": "object",
      "properties": {
        "symbol": {
          "type": "string",
          "title": "선물 종목 심볼"
        },
        "multiple": {
          "type": "string",
          "title": "승수"
        },
        "lastAskPrice": {
          "type": "string",
          "title": "최근 매도호가"
        },
        "lastBidPrice": {
          "type": "string",
          "title": "최근 매수호가"
        }
      },
      "title": "선물형 기초자산"
    },
    "protobufAny": {
      "type": "object",
      "properties": {
        "@type": {
          "type": "string"
        }
      },
      "additionalProperties": {}
    },
    "rpcStatus": {
      "type": "object",
      "properties": {
        "code": {
          "type": "integer",
          "format": "int32"
        },
        "message": {
          "type": "string"
        },
        "details": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/protobufAny"
          }
        }
      }
    }
  }
}
