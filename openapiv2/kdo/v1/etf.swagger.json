        "tickSize": {
          "type": "string",
          "format": "int64",
          "title": "Tick 크기 (원 단위, i64)"
        },
        "listedQuantity": {
          "type": "string",
          "format": "int64",
          "title": "Tick 크기 (원 단위, i64)"
        },
        "leverage": {
          "type": "number",
          "format": "float"
        },
        "tradable": {
          "type": "boolean"
        },
        "shortSellable": {
          "type": "boolean"
        },
        "cashCreditable": {
          "type": "boolean"
        },
        "cashCreationAmount": {
          "type": "string",
          "format": "int64"
        }
      }
    },
    "etfEtfPdfConstituent": {
      "type": "object",
      "properties": {
        "symbol": {
          "type": "string",
          "description": "symbol",
          "title": "종목 코드"
        },
        "name": {
          "type": "string",
          "title": "종목명"
        },
        "productType": {
          "$ref": "#/definitions/etfProductType",
          "title": "상품 타입"
        },
        "quantity": {
          "type": "string",
          "format": "int64",
          "title": "구성 수량 (선물 숏의 경우 음수)"
        }
      },
      "title": "PDF 구성 종목"
    },
    "etfListEtfsResponse": {
      "type": "object",
      "properties": {
        "etfs": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/etfEtf"
          },
          "title": "ETF 목록"
        },
        "nextPageToken": {
          "type": "string",
          "title": "다음 페이지 토큰"
        }
      }
    },
    "etfProductType": {
      "type": "string",
      "enum": [
        "PRODUCT_TYPE_UNSPECIFIED",
        "PRODUCT_TYPE_STOCK",
        "PRODUCT_TYPE_FUTURES",
        "PRODUCT_TYPE_ETF",
        "PRODUCT_TYPE_CASH"
      ],
      "default": "PRODUCT_TYPE_UNSPECIFIED",
      "title": "상품 타입"
    },
    "etfReplicationMethod": {
      "type": "string",
      "enum": [
        "REPLICATION_METHOD_UNSPECIFIED",
        "REPLICATION_METHOD_ACTIVE",
        "REPLICATION_METHOD_PHYSICAL",
        "REPLICATION_METHOD_SYNTHETIC"
      ],
      "default": "REPLICATION_METHOD_UNSPECIFIED",
      "title": "복제 방법"
    },
    "protobufAny": {
      "type": "object",
      "properties": {
        "@type": {
          "type": "string"
        }
      },
      "additionalProperties": {}
    },
    "rpcStatus": {
      "type": "object",
      "properties": {
        "code": {
          "type": "integer",
          "format": "int32"
        },
        "message": {
          "type": "string"
        },
        "details": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/protobufAny"
          }
        }
      }
    }
  }
