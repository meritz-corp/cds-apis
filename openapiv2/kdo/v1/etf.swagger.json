        },
        "code": {
          "type": "string",
          "title": "ETF 코드"
        },
        "name": {
          "type": "string",
          "title": "ETF 이름"
        },
        "prevClose": {
          "type": "string",
          "title": "마지막 가격"
        },
        "lastPrice": {
          "type": "string"
        },
        "prevNav": {
          "type": "string"
        },
        "lastInav": {
          "type": "string"
        },
        "constituents": {
          "type": "object",
          "additionalProperties": {
            "$ref": "#/definitions/etfEtfPdfConstituent"
          },
          "title": "PDF 구성 종목 목록"
        },
        "creationUnit": {
          "type": "string",
