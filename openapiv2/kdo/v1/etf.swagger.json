{
  "swagger": "2.0",
  "info": {
    "title": "kdo/v1/etf.proto",
    "version": "version not set"
  },
  "tags": [
    {
      "name": "EtfService"
    }
  ],
  "consumes": [
    "application/json"
  ],
  "produces": [
    "application/json"
  ],
  "paths": {
    "/v1/etfs": {
      "get": {
        "operationId": "EtfService_ListEtfs",
        "responses": {
          "200": {
            "description": "A successful response.",
            "schema": {
              "$ref": "#/definitions/etfListEtfsResponse"
            }
          },
          "default": {
            "description": "An unexpected error response.",
            "schema": {
              "$ref": "#/definitions/rpcStatus"
            }
          }
        },
        "parameters": [
          {
            "name": "pageSize",
            "description": "페이지 크기 (optional)",
            "in": "query",
            "required": false,
            "type": "integer",
            "format": "int64"
          },
          {
            "name": "pageToken",
            "description": "페이지 토큰 (optional, for pagination)",
            "in": "query",
            "required": false,
            "type": "string"
          },
          {
            "name": "filter",
            "description": "필터링 조건 (optional, AIP-160)",
            "in": "query",
            "required": false,
            "type": "string"
          }
        ],
        "tags": [
          "EtfService"
        ]
      }
    },
    "/v1/etfs:create_redeem": {
      "post": {
        "operationId": "EtfService_CreateRedeemEtf",
        "responses": {
          "200": {
            "description": "A successful response.",
            "schema": {
              "$ref": "#/definitions/etfEtf"
            }
          },
          "default": {
            "description": "An unexpected error response.",
            "schema": {
              "$ref": "#/definitions/rpcStatus"
            }
          }
        },
        "parameters": [
          {
            "name": "body",
            "in": "body",
            "required": true,
            "schema": {
              "$ref": "#/definitions/etfCreateRedeemEtfRequest"
            }
          }
        ],
        "tags": [
          "EtfService"
        ]
      }
    },
    "/v1/{etf}": {
      "get": {
        "operationId": "EtfService_GetEtf",
        "responses": {
          "200": {
            "description": "A successful response.",
            "schema": {
              "$ref": "#/definitions/etfEtf"
            }
          },
          "default": {
            "description": "An unexpected error response.",
            "schema": {
              "$ref": "#/definitions/rpcStatus"
            }
          }
        },
        "parameters": [
          {
            "name": "etf",
            "in": "path",
            "required": true,
            "type": "string",
            "pattern": "etfs/[^/]+"
          }
        ],
        "tags": [
          "EtfService"
        ]
      }
    }
  },
  "definitions": {
    "etfCreateRedeemEtfRequest": {
      "type": "object",
      "properties": {
        "etf": {
          "type": "string"
        },
        "fund": {
          "type": "string"
        },
        "quantity": {
          "type": "string",
          "format": "int64"
        },
        "isCreation": {
          "type": "boolean"
        }
      },
      "required": [
        "etf",
        "fund",
        "quantity",
        "isCreation"
      ]
    },
    "etfEtf": {
      "type": "object",
      "properties": {
        "id": {
          "type": "string",
          "format": "uint64",
