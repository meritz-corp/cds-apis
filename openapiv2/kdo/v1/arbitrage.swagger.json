        "state": {
          "$ref": "#/definitions/arbitrageArbitrageState",
          "title": "현재 상태"
        },
        "basketAValue": {
          "type": "string",
          "format": "int64",
          "title": "바스켓 A 현재 가치"
        },
        "basketBValue": {
          "type": "string",
          "format": "int64",
          "title": "바스켓 B 현재 가치"
        },
        "currentSpread": {
          "type": "string",
          "format": "int64",
          "title": "현재 스프레드"
        },
        "currentSpreadBps": {
          "type": "number",
          "format": "double",
          "title": "현재 스프레드 (bps)"
        },
