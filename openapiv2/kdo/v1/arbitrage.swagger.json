        "basketAId",
        "basketBId"
      ]
    },
    "arbitrageArbitrageEvent": {
      "type": "object",
      "properties": {
        "name": {
          "type": "string",
          "title": "리소스 이름 (arbitrages/{id})"
        },
        "id": {
          "type": "integer",
          "format": "int32",
          "title": "차익거래 ID"
        },
        "stateChanged": {
          "$ref": "#/definitions/arbitrageStateChangedEvent"
        },
        "priceUpdate": {
          "$ref": "#/definitions/arbitragePriceUpdateEvent"
        },
        "triggered": {
          "$ref": "#/definitions/v1arbitrageTriggeredEvent"
        },
        "roundStarted": {
          "$ref": "#/definitions/arbitrageRoundStartedEvent"
        },
        "orderSubmitted": {
          "$ref": "#/definitions/arbitrageOrderSubmittedEvent"
        },
        "orderFilled": {
          "$ref": "#/definitions/arbitrageOrderFilledEvent"
        },
        "roundCompleted": {
          "$ref": "#/definitions/arbitrageRoundCompletedEvent"
        },
        "executionCompleted": {
          "$ref": "#/definitions/arbitrageExecutionCompletedEvent"
        },
        "error": {
          "$ref": "#/definitions/arbitrageErrorEvent"
        },
        "basketPrice": {
          "$ref": "#/definitions/arbitrageBasketPriceUpdateEvent"
        }
      },
      "title": "차익거래 이벤트"
    },
    "arbitrageArbitrageSide": {
      "type": "string",
      "enum": [
        "ARBITRAGE_SIDE_UNSPECIFIED",
        "ARBITRAGE_SIDE_BUY_A_SELL_B",
        "ARBITRAGE_SIDE_BUY_B_SELL_A"
      ],
      "default": "ARBITRAGE_SIDE_UNSPECIFIED",
      "description": "- ARBITRAGE_SIDE_BUY_A_SELL_B: A 매수, B 매도\n - ARBITRAGE_SIDE_BUY_B_SELL_A: B 매수, A 매도",
      "title": "차익거래 방향"
    },
    "arbitrageArbitrageState": {
      "type": "string",
      "enum": [
        "ARBITRAGE_STATE_UNSPECIFIED",
        "ARBITRAGE_STATE_IDLE",
        "ARBITRAGE_STATE_MONITORING",
        "ARBITRAGE_STATE_TRIGGERED",
        "ARBITRAGE_STATE_EXECUTING",
        "ARBITRAGE_STATE_STOPPED",
        "ARBITRAGE_STATE_ERROR"
      ],
      "default": "ARBITRAGE_STATE_UNSPECIFIED",
      "description": "- ARBITRAGE_STATE_IDLE: 대기 중 (등록됨, 미시작)\n - ARBITRAGE_STATE_MONITORING: 모니터링 중 (시세 수신 및 트리거 평가)\n - ARBITRAGE_STATE_TRIGGERED: 트리거 발동됨 (조건 충족, 실행 준비)\n - ARBITRAGE_STATE_EXECUTING: 실행 중 (주문 진행 중)\n - ARBITRAGE_STATE_STOPPED: 중지됨\n - ARBITRAGE_STATE_ERROR: 에러 상태",
      "title": "차익거래 런타임 상태"
    },
    "arbitrageArbitrageStats": {
      "type": "object",
      "properties": {
        "triggerCount": {
          "type": "string",
          "format": "uint64",
          "title": "트리거 발동 횟수"
        },
        "executionCount": {
          "type": "string",
          "format": "uint64",
          "title": "실행 완료 횟수"
        },
        "executionFailures": {
          "type": "string",
          "format": "uint64",
          "title": "실행 실패 횟수"
        },
        "totalProfit": {
          "type": "string",
          "format": "int64",
          "title": "총 수익"
        }
      },
      "title": "차익거래 통계"
    },
    "arbitrageArbitrageStatus": {
      "type": "object",
      "properties": {
        "name": {
          "type": "string",
          "title": "리소스 이름 (arbitrages/{id})"
