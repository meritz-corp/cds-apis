{
  "swagger": "2.0",
  "info": {
    "title": "kdo/v1/notification.proto",
    "version": "version not set"
  },
  "tags": [
    {
      "name": "NotificationService"
    }
  ],
  "consumes": [
    "application/json"
  ],
  "produces": [
    "application/json"
  ],
  "paths": {
    "/v1/{name}:ack": {
      "post": {
        "summary": "AcknowledgeNotification marks a notification as acknowledged.",
        "operationId": "NotificationService_AcknowledgeNotification",
        "responses": {
          "200": {
            "description": "A successful response.",
            "schema": {
              "type": "object",
              "properties": {}
            }
          },
          "default": {
            "description": "An unexpected error response.",
            "schema": {
              "$ref": "#/definitions/rpcStatus"
            }
          }
        },
        "parameters": [
          {
            "name": "name",
            "description": "The resource name of the notification.\nFormat: users/{user_id}/notifications/{notification_id}",
            "in": "path",
            "required": true,
            "type": "string",
            "pattern": "users/[^/]+/notifications/[^/]+"
          },
          {
            "name": "body",
            "in": "body",
            "required": true,
            "schema": {
              "type": "object",
              "title": "Request message for AcknowledgeNotification"
            }
          }
        ],
        "tags": [
          "NotificationService"
        ]
      }
    },
    "/v1/{parent}/notifications": {
      "get": {
        "summary": "ListNotifications returns a list of notifications for a user.",
        "operationId": "NotificationService_ListNotifications",
        "responses": {
          "200": {
            "description": "A successful response.",
            "schema": {
              "$ref": "#/definitions/notificationListNotificationsResponse"
            }
          },
          "default": {
            "description": "An unexpected error response.",
            "schema": {
              "$ref": "#/definitions/rpcStatus"
            }
          }
        },
        "parameters": [
          {
            "name": "parent",
            "description": "The resource name of User.\nFormat: users/{user_id}",
            "in": "path",
            "required": true,
            "type": "string",
            "pattern": "users/[^/]+"
          },
          {
            "name": "pageSize",
            "description": "The maximum number of items to return.\nIf unspecified, at most 50 rows will be returned.\nThe maximum value is 1024; values above 1024 will be coerced to 1024.",
            "in": "query",
            "required": false,
            "type": "integer",
            "format": "int32"
          },
          {
            "name": "pageToken",
            "description": "Token of the page to retrieve.",
            "in": "query",
            "required": false,
            "type": "string"
          },
          {
            "name": "filter",
            "description": "Filter expression.\nAvailable filters:\n* state: CREATED, ACKNOWLEDGED, EXPIRED\n* type: INFO, ERROR\n\nExamples:\n* state=CREATED\n* type=INFO",
            "in": "query",
            "required": false,
            "type": "string"
          }
        ],
        "tags": [
          "NotificationService"
        ]
      }
    },
    "/v1/{parent}/notifications:subscribe": {
      "get": {
        "summary": "SubscribeNotifications subscribes to real-time notifications for a user.",
        "operationId": "NotificationService_SubscribeNotifications",
        "responses": {
          "200": {
            "description": "A successful response.(streaming responses)",
            "schema": {
              "type": "object",
              "properties": {
                "result": {
                  "$ref": "#/definitions/notificationNotification"
                },
                "error": {
                  "$ref": "#/definitions/rpcStatus"
                }
              },
              "title": "Stream result of notificationNotification"
            }
          },
          "default": {
            "description": "An unexpected error response.",
            "schema": {
              "$ref": "#/definitions/rpcStatus"
            }
          }
        },
        "parameters": [
          {
            "name": "parent",
            "description": "The resource name of User.\nFormat: users/{user_id}",
            "in": "path",
            "required": true,
            "type": "string",
            "pattern": "users/[^/]+"
          }
        ],
        "tags": [
          "NotificationService"
        ]
      }
    }
  },
  "definitions": {
    "notificationListNotificationsResponse": {
      "type": "object",
      "properties": {
        "notifications": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/notificationNotification"
          }
        },
        "nextPageToken": {
          "type": "string"
        }
      },
      "title": "Response message for ListNotifications"
    },
    "notificationLpPageTarget": {
      "type": "object",
      "properties": {
        "etfCode": {
          "type": "string",
          "title": "ETF code (e.g., \"A252670\")"
        },
        "fundCode": {
          "type": "string",
          "title": "Fund code (e.g., \"0331\")"
        }
      },
      "title": "LP page navigation target"
    },
    "notificationNavigateAction": {
      "type": "object",
      "properties": {
        "lpPage": {
          "$ref": "#/definitions/notificationLpPageTarget",
          "description": "Future targets can be added here:\n OrderDetailTarget order_detail = 2;\n SettingsTarget settings = 3;",
          "title": "Navigate to LP page"
        }
      },
      "title": "Navigation action"
    },
    "notificationNoOp": {
      "type": "object",
      "title": "No operation action"
    },
    "notificationNotification": {
      "type": "object",
      "properties": {
        "name": {
          "type": "string",
          "title": "The resource name of the Notification.\nFormat: users/{user_id}/notifications/{notification_id}"
        },
        "user": {
          "type": "string",
          "title": "The resource name of User.\nFormat: users/{user_id}",
          "readOnly": true
        },
        "id": {
          "type": "string",
          "format": "uint64",
          "description": "The id of Notification.",
          "readOnly": true
        },
        "type": {
          "$ref": "#/definitions/notificationNotificationType",
          "title": "Notification type",
          "readOnly": true
        },
        "action": {
          "$ref": "#/definitions/notificationNotificationAction",
          "title": "Action to perform when clicked",
          "readOnly": true
        },
        "state": {
          "$ref": "#/definitions/notificationNotificationState",
          "title": "Current state",
          "readOnly": true
        },
        "title": {
          "type": "string",
          "title": "Notification title",
          "readOnly": true
        },
        "body": {
          "type": "string",
          "title": "Notification body",
          "readOnly": true
        },
        "metadata": {
          "type": "object",
          "additionalProperties": {
            "type": "string"
          },
          "title": "Additional metadata",
          "readOnly": true
        },
        "navigationUrl": {
          "type": "string",
          "title": "Pre-computed navigation URL (convenience field)\ne.g., \"/lp/A252670\"",
          "readOnly": true
        },
        "createTime": {
          "type": "string",
          "format": "date-time",
          "title": "Created timestamp",
          "readOnly": true
        },
        "expireTime": {
          "type": "string",
          "format": "date-time",
          "title": "Expiration timestamp (if applicable)",
          "readOnly": true
        },
        "acknowledgeTime": {
          "type": "string",
          "format": "date-time",
          "title": "Acknowledged timestamp",
          "readOnly": true
        }
      },
      "title": "Notification resource"
    },
    "notificationNotificationAction": {
      "type": "object",
      "properties": {
        "noOp": {
          "$ref": "#/definitions/notificationNoOp",
          "title": "No operation"
        },
        "navigate": {
          "$ref": "#/definitions/notificationNavigateAction",
          "title": "Navigate to a page"
        }
      },
      "title": "Action to perform when notification is clicked"
    },
    "notificationNotificationState": {
      "type": "string",
      "enum": [
        "STATE_UNSPECIFIED",
        "CREATED",
        "ACKNOWLEDGED",
        "EXPIRED"
      ],
      "default": "STATE_UNSPECIFIED",
      "description": "- CREATED: Notification has been created\n - ACKNOWLEDGED: Notification has been acknowledged by user\n - EXPIRED: Notification has expired",
      "title": "State of the notification"
    },
    "notificationNotificationType": {
      "type": "string",
      "enum": [
        "TYPE_UNSPECIFIED",
        "INFO",
        "ERROR"
      ],
      "default": "TYPE_UNSPECIFIED",
      "description": "- INFO: Informational notification\n - ERROR: Error notification",
      "title": "Type of the notification"
    },
    "protobufAny": {
      "type": "object",
      "properties": {
        "@type": {
          "type": "string"
        }
      },
      "additionalProperties": {}
    },
    "rpcStatus": {
      "type": "object",
      "properties": {
        "code": {
          "type": "integer",
          "format": "int32"
        },
        "message": {
          "type": "string"
        },
        "details": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/protobufAny"
          }
        }
      }
    }
  }
}
