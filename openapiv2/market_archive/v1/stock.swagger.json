  "paths": {
    "/v1/stocks": {
      "get": {
        "operationId": "StockService_ListStocks",
        "responses": {
          "200": {
            "description": "A successful response.",
            "schema": {
              "$ref": "#/definitions/stockListStocksResponse"
            }
          },
          "default": {
            "description": "An unexpected error response.",
            "schema": {
              "$ref": "#/definitions/rpcStatus"
            }
          }
        },
        "parameters": [
          {
            "name": "pageSize",
            "description": "페이지 크기 (optional)",
            "in": "query",
            "required": false,
            "type": "integer",
            "format": "int64"
          },
          {
            "name": "pageToken",
            "description": "페이지 토큰 (optional, for pagination)",
            "in": "query",
            "required": false,
            "type": "string"
          },
          {
            "name": "filter",
            "description": "필터링 조건 (optional, AIP-160)",
            "in": "query",
            "required": false,
            "type": "string"
          }
        ],
        "tags": [
          "StockService"
        ]
      }
    },
    "/v1/{stock}": {
      "get": {
        "operationId": "StockService_GetStock",
        "responses": {
          "200": {
            "description": "A successful response.",
            "schema": {
              "$ref": "#/definitions/stockStock"
            }
          },
          "default": {
            "description": "An unexpected error response.",
            "schema": {
              "$ref": "#/definitions/rpcStatus"
            }
          }
        },
        "parameters": [
          {
            "name": "stock",
            "in": "path",
            "required": true,
            "type": "string",
            "pattern": "stocks/[^/]+"
          }
        ],
        "tags": [
          "StockService"
        ]
      }
    }
  },
