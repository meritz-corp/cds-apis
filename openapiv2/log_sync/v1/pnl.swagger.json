        },
        "totalPnl": {
          "type": "string",
          "title": "총 손익 (문자열로 표현된 decimal)"
        },
        "totalVolume": {
          "type": "string",
          "title": "총 거래량 (문자열로 표현된 decimal)"
        },
        "totalTransactions": {
          "type": "string",
          "format": "int64",
