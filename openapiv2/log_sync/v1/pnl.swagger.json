  "paths": {
    "/v1/pnl/summaries": {
      "get": {
        "summary": "ListPnlSummaries will return pnl summaries.",
        "operationId": "PnlService_ListPnlSummaries",
        "responses": {
          "200": {
            "description": "A successful response.",
            "schema": {
              "$ref": "#/definitions/pnlListPnlSummariesResponse"
            }
          },
          "default": {
            "description": "An unexpected error response.",
            "schema": {
              "$ref": "#/definitions/rpcStatus"
            }
          }
        },
        "parameters": [
          {
            "name": "pageSize",
            "description": "The maximum number of pnl summaries to return. The service may return fewer than\nthis value.\nIf unspecified, at most 50 rows will be returned.\nThe maximum value is 1024; values above 1024 will be coerced to 1024.",
            "in": "query",
            "required": false,
            "type": "integer",
            "format": "int32"
          },
          {
            "name": "pageToken",
            "description": "Token of the page to retrieve. If not specified, the first\npage of results will be returned. Use the value obtained from\n`next_page_token` in the previous response to request\nthe next page of results.\n\nWhen paginating, all other parameters provided to `ListPnlSummaries` must match\nthe call that provided the page token.",
            "in": "query",
            "required": false,
            "type": "string"
          },
          {
            "name": "filter",
            "description": "Available Sequence and Operator\n* start_date\n  * `\u003e`, `\u003e=`, `=`, `\u003c=`, `\u003c`\n* end_date\n  * `\u003e`, `\u003e=`, `=`, `\u003c=`, `\u003c`\n* period_type\n  * `equal`\n\nExamples\n* filter=start_date\u003e\"2025-01-01T00\" AND end_date\u003c\"2025-01-31T23\" \n* filter=period_type=PERIOD_TYPE_MONTHLY",
            "in": "query",
            "required": false,
            "type": "string"
          }
        ],
        "tags": [
          "PnlService"
        ]
      }
    }
  },
  "definitions": {
    "pnlListPnlSummariesResponse": {
      "type": "object",
      "properties": {
        "summaries": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/pnlPnlSummary"
          },
          "description": "The list of rows that matched the query."
        },
        "nextPageToken": {
          "type": "string",
          "description": "Pagination token used to retrieve the next page of results.\nPass the content of this string as the `page_token` attribute of\nthe next request. `next_page_token` is not returned for the last\npage."
        }
      },
      "title": "ListPnlSummariesResponse response"
    },
    "pnlPeriodType": {
      "type": "string",
      "enum": [
        "PERIOD_TYPE_UNSPECIFIED",
        "PERIOD_TYPE_DAILY",
        "PERIOD_TYPE_WEEKLY",
        "PERIOD_TYPE_MONTHLY",
        "PERIOD_TYPE_YEARLY"
      ],
      "default": "PERIOD_TYPE_UNSPECIFIED",
      "title": "- PERIOD_TYPE_UNSPECIFIED: Unspecified period type\n - PERIOD_TYPE_DAILY: Daily period\n - PERIOD_TYPE_WEEKLY: Weekly period\n - PERIOD_TYPE_MONTHLY: Monthly period\n - PERIOD_TYPE_YEARLY: Yearly period"
    },
    "pnlPnlSummary": {
      "type": "object",
